  "lint-staged": {
    "app/**/*.{json,ts,tsx,css,scss}": [
      "prettier --write",
      "git add"
    ],
    "lib/**/*.{json,ts,tsx,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "npm run webpack:prod",
    "prettier:format": "prettier --write \"lib/**/*.{json,ts,tsx,css,scss}\"",
    "lint": "tslint --project tsconfig.json -e 'node_modules/**'",
    "lint:fix": "npm run lint -- --fix",
    "cleanup": "rimraf target/www dist",
    "start": "npm run webpack:dev",
    "start-tls": "npm run webpack:dev -- --env.tls",
    "test": "npm run lint && jest --coverage --logHeapUsage -w=2 --config test/javascript/jest.conf.js",
    "test:watch": "npm test -- --watch",
    "webpack:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --inline --port=9060 --env.stats=minimal",
    "webpack:dev-verbose": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --inline --port=9060 --profile --progress --env.stats=normal",
    "webpack:build:main": "npm run webpack -- --config webpack/webpack.dev.js --env.stats=minimal",
    "webpack:build": "npm run cleanup && npm run webpack:build:main",
    "webpack:prod:main": "npm run webpack --max_old_space_size=4096 -- --config webpack/webpack.prod.js --max_old_space_size=4096 --profile",
    "webpack:prod": "npm run cleanup && npm run webpack:prod:main",
    "webpack:test": "npm run test",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "lib:stage": "cp -r lib dist && cp tsconfig-lib.json dist/tsconfig.json && cp -r static dist/ && ln -s ../node_modules dist/node_modules && cp package.json dist/",
    "lib:importfix": "cd dist/; find . -type f | xargs sed -i 's/..\\/static/static/g'",
    "lib:typescript": "cd dist/; tsc; rm node_modules",
    "lib": "npm run cleanup && npm run lib:stage && npm run lib:importfix && npm run lib:typescript"
  },
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "snyk": true
